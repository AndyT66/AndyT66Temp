package uk.ac.lancs.e_science.sakaiproject.api.blogger.util;

public class Par {
	private Object _objeto1;
	private Object _objeto2;
	
	public Par() {
	}
	
	public Par(Object objeto1, Object objeto2) {
		_objeto1 = objeto1;
		_objeto2 = objeto2;
	}
	
	

	/**
	 * Returns the objeto1.
	 * @return Object
	 */
	public Object getObjeto1() {
		return _objeto1;
	}

	/**
	 * Returns the objeto2.
	 * @return Object
	 */
	public Object getObjeto2() {
		return _objeto2;
	}

	/**
	 * Sets the objeto1.
	 * @param objeto1 The objeto1 to set
	 */
	public void setObjeto1(Object objeto1) {
		_objeto1 = objeto1;
	}

	/**
	 * Sets the objeto2.
	 * @param objeto2 The objeto2 to set
	 */
	public void setObjeto2(Object objeto2) {
		_objeto2 = objeto2;
	}
	
	/**
	 * Compara dos pares.
	 * 
	 * @param par Par que se desea comparar.
	 * 
	 * @return Retorna <true>si los dos objetos son iguales, <code>false</code> si no.
	 */
	public boolean equals(Par par) {
		return ((getObjeto1().equals(par.getObjeto1())) && getObjeto2().equals(par.getObjeto2()));
	}

}
