<?xml version="1.0"?>
<project xmlns="http://maven.apache.org/POM/4.0.0">
  <modelVersion>4.0.0</modelVersion>
  <parent>
    <artifactId>blog-base</artifactId>
    <groupId>org.sakaiproject</groupId>
    <version>2.6.1</version>
    <relativePath>../pom.xml</relativePath>
  </parent>
  <name>sakai-blogger-tool</name>
  <artifactId>sakai-blogger-tool</artifactId>
  <groupId>org.sakaiproject</groupId>
  <organization>
    <name>Lancaster Centre for e-Science</name>
    <url>http://e-science.lancs.ac.uk</url>
  </organization>
  <inceptionYear>2005</inceptionYear>
  <developers>
    <developer>
      <name>Adrian Fish</name>
      <email>a.fish@lancaster.ac.uk</email>
    </developer>
    <developer>
      <name>Miguel Gonzalez Losa</name>
      <email>m.gonzalez@lancaster.ac.uk</email>
    </developer>
  </developers>
  <packaging>war</packaging>
  <dependencies>
    <dependency>
      <groupId>org.sakaiproject.kernel</groupId>
      <artifactId>sakai-kernel-util</artifactId>
    </dependency>
    <dependency>
      <groupId>org.sakaiproject</groupId>
      <artifactId>sakai-blogger-api</artifactId>
      <version>${sakai.version}</version>
      <type>jar</type>
    </dependency>
    <dependency>
      <groupId>org.sakaiproject</groupId>
      <artifactId>sakai-blogger-api-impl</artifactId>
      <version>${sakai.version}</version>
      <type>jar</type>
    </dependency>
    <dependency>
      <groupId>org.sakaiproject</groupId>
      <artifactId>sakai-blogger-fileUpload</artifactId>
      <version>${sakai.version}</version>
      <type>jar</type>
    </dependency>
    <dependency>
      <groupId>org.sakaiproject</groupId>
      <artifactId>sakai-blogger-jsf</artifactId>
      <version>${sakai.version}</version>
      <type>jar</type>
    </dependency>
    <dependency>
      <groupId>org.sakaiproject.kernel</groupId>
      <artifactId>sakai-kernel-api</artifactId>
    </dependency>
    <dependency>
      <groupId>org.sakaiproject.kernel</groupId>
      <artifactId>sakai-component-manager</artifactId>
    </dependency>
<!--
      This has been relocated to sakai-kernel-util
      org.sakaiproject.kernel.utilsakai-entity-util-->
    <dependency>
      <groupId>org.sakaiproject</groupId>
      <artifactId>sakai-jsf-sun-ri-tool</artifactId>
      <version>${sakai.version}</version>
      <type>pom</type>
    </dependency>
    <dependency>
      <groupId>commons-fileupload</groupId>
      <artifactId>commons-fileupload</artifactId>
      <version>1.1.1</version>
      <type>jar</type>
    </dependency>
    <dependency>
      <groupId>org.apache.myfaces.tomahawk</groupId>
      <artifactId>tomahawk</artifactId>
      <version>1.1.6</version>
      <type>jar</type>
      <exclusions>
        <exclusion>
<!-- shared -->
          <groupId>antlr</groupId>
          <artifactId>antlr</artifactId>
        </exclusion>
      </exclusions>
    </dependency>
    <dependency>
      <groupId>javax.servlet</groupId>
      <artifactId>servlet-api</artifactId>
      <version>${sakai.servletapi.version}</version>
      <type>jar</type>
      <scope>provided</scope>
    </dependency>
  </dependencies>
  <build>
    <resources>
      <resource>
        <directory>${basedir}/src/bundle</directory>
        <includes>
          <include>**/*.properties</include>
        </includes>
      </resource>
    </resources>
<!-- other resources for the jar - the message bundles -->
  </build>
</project>
